{
  "name": "pdf-generator",
  "version": "0.0.2",
  "engines": {
    "npm": ">=8",
    "node": "18"
  },
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "lint": "prettier --check . && eslint --ignore-path .gitignore .",
    "format": "prettier --write . && eslint --ignore-path .gitignore --fix .",
    "prepare": "husky install"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "bottleneck": "^2.19.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "helmet": "7.0.0",
    "pino": "^10.0.0",
    "pino-http": "^11.0.0",
    "puppeteer": "21.2.1",
    "tmp": "0.2.1"
  },
  "devDependencies": {
    "eslint": "8.49.0",
    "eslint-config-eslint": "9.0.0",
    "eslint-config-prettier": "9.0.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "pino-pretty": "^13.1.2",
    "prettier": "3.0.3"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write --plugin-search-dir=.",
      "eslint --ignore-path .gitignore --fix"
    ]
  }
}
